@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
</head>
<body>

    @*Header*@
    @*Html Tag*@
    <ul class="nav justify-content-center  mt-4">
        <li class="nav-item">
            <a class="  btn btn-success " href="/Home/Index">Home</a>
        </li>
        <li class="nav-item ms-3">
            <a class="  btn  btn-primary " href="/Home/AboutUs">About Us</a>
        </li>
        <li class="nav-item ms-3">
            <a class=" btn  btn-info " href="/Home/Privacy">Privacy</a>
        </li>
        <li class="nav-item">
            <a class="nav-link disabled" aria-disabled="true">Disabled</a>
        </li>
        @*Tag Helper*@
        <li class="nav-item">
            <a class="nav-link" asp-Controller="Home" asp-action="Index">Homme</a>
          </li>
            @*Header*@
    </ul>
    @*HTML Helper*@
    @*Html.ActionLink("Homme","Index","Home")*@

    <div>
        @RenderBody()
    </div>
    @*Footer*@
    <footer>
        <div class="mt-4 p-4 justify-content-center text-center fixed-bottom">
            <p>CopyRight &copy; @DateTime.Now.Year</p>
        </div>
    </footer>
    @*Footer*@
</body>
</html>
